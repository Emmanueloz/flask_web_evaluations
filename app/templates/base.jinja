<!DOCTYPE html>
<html>

<head>
    <title>{% block title %}{% endblock title %}</title>
    {% block styles %}
    {% endblock styles %}
</head>

<body>
    <header>
        {% block header %}
        {% endblock header %}
    </header>
    {% for category,message in get_flashed_messages(with_categories=true) %}
    <div class="flash flash-{{category}}">{{ message }}</div>
    {% endfor %}
    {% block content %}
    {% endblock content %}
    {% block scripts %}
    <script type="module" src="{{url_for('static',filename='js/token.js')}}"></script>
    {% endblock scripts %}
</body>

</html>